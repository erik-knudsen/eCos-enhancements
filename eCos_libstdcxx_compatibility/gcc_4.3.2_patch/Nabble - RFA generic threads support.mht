Nabble.deletePost =3D function(postId) {
	if( !confirm("Are you sure you want to permanently delete this message =
and all of its replies?") )
		return;
	top.location =3D =
"/forum/DeletePost.jtp?post=3D"+postId+"&pwd=3D"+this.getCookie("password=
");
};


// flagging

Nabble.setFlag =3D function(postId) {
	if( this.user=3D=3Dnull ) {
		if( confirm(
			"You must be logged in to flag a message.\n"
			+"Would you like to login now?"
		) ) {
			top.location =3D =
"/user/Login.jtp?nextUrl=3D"+encodeURIComponent(top.location);
		}
		return;
	}
	this.isFlagged[postId] =3D !this.isFlagged[postId];
	top.location =3D "/forum/Flag.jtp?post=3D" + postId + "&flag=3D" + =
this.isFlagged[postId];
	this.showFlag(postId);
};

Nabble.flaggedImg =3D new Image();
Nabble.flaggedImg.src =3D "/images/icon_flagged.png";
Nabble.unflaggedImg =3D new Image();
Nabble.unflaggedImg.src =3D "/images/icon_unflagged.png";

Nabble.showFlag =3D function(postId) {
	var img =3D this.get("nabble.flag"+postId);
	if( this.isFlagged[postId] ) {
		img.src =3D this.flaggedImg.src;
		img.alt =3D "Flagged, click to clear flag";
	} else {
		img.src =3D this.unflaggedImg.src;
		img.alt =3D "Click to flag this post";
	}
	this.get("nabble.aflag"+postId).title =3D img.alt;
}


// rating

Nabble.emptyStar =3D new Image();
Nabble.emptyStar.src =3D "/images/icon_star_grey.png";
Nabble.ratedStar =3D new Image();
Nabble.ratedStar.src =3D "/images/icon_star_red.png";
Nabble.predStar =3D new Image();
Nabble.predStar.src =3D "/images/icon_star_blue.png";

Nabble.clicked =3D function(postId,n) {
	if( this.user=3D=3Dnull ) {
		if( confirm(
			"You must be logged in to rate a post.\n"
			+"Would you like to login now?"
		) ) {
			top.location =3D =
"/user/Login.jtp?nextUrl=3D"+encodeURIComponent(top.location);
		}
		return;
	}
	this.ratedStars[postId] =3D n;
	location =3D =
"/rating/SetRating.jtp?post=3D"+postId+"&rating=3D"+n+"&pwd=3D"+this.getC=
ookie("password");
};

Nabble.mouseover =3D function(postId,n) {
	for( i=3D1; i<=3Dn; i++ ) {
		this.get('nabble.star'+postId+'-'+i).src =3D this.ratedStar.src;
	}
	for( ; i<=3D5; i++ ) {
		this.get('nabble.star'+postId+'-'+i).src =3D this.emptyStar.src;
	}
	status =3D this.get('nabble.star'+postId+'-'+n).alt;
	this.get("nabble.descriptor"+postId).innerHTML =3D [
		"Clear Rating",
		"Poor Post",
		"Below Average Post",
		"Average Post",
		"Above Average Post",
		"Excellent Post",
	][n];
};

Nabble.mouseout =3D function(postId) {
	for( i=3D1; i<=3Dthis.ratedStars[postId]; i++ ) {
		this.get('nabble.star'+postId+'-'+i).src =3D this.ratedStar.src;
	}
	for( ; i<=3D5; i++ ) {
		this.get('nabble.star'+postId+'-'+i).src =3D this.emptyStar.src;
	}
	status =3D defaultStatus;
	this.get("nabble.descriptor"+postId).innerHTML =3D '- Use ratings to =
moderate (<a title=3D"Click for more info" target=3D"_top" =
href=3D"http://www.nabble.com/help/Answer.jtp?id=3D16">?</a>)';
};

Nabble.setEditAlert =3D function(alertId) {
    var link =3D this.get('nabble.alertlink');
    link.href =3D =
"/alerts/EmailAlerts.jtp?id=3D"+alertId+"&from=3Dmodify&back=3D"+encodeUR=
IComponent(top.location);
    link.innerHTML =3D 'Edit email alert';
};

Nabble.searchterms =3D Nabble.getSearchterms();
if( Nabble.gquery!=3Dnull && Nabble.hideAd )
	Nabble.hasAdSense =3D false;

Nabble.tview =3D "dump";

Nabble.setView =3D function(newView,post) {
	Nabble.setVar("tview",newView);
	var url =3D =
"http://www.nabble.com/RFA%3A-generic-threads-support-tf1338578.html";
	if( post !=3D null )
		url +=3D "#a" + post;
	top.location.replace(url);
};

Nabble.cur =3D location.hash ? location.hash.substr(2) : null;

Nabble.setMarker =3D function(post) {
	if( Nabble.cur ) {
		var t =3D Nabble.get( "nabble.i" + Nabble.cur );
		if( t ) {
			t.style.visibility =3D "hidden";
		}
	}
	Nabble.cur =3D post;
	var t =3D Nabble.get( "nabble.i" + Nabble.cur );
	if( t ) {
		t.style.visibility =3D "visible";
	}
}

